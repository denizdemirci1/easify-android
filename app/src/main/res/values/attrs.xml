<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SearchView">
        <attr name="hint" format="string" />
        <attr name="hintColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FeatureView">
        <attr name="editable" format="boolean" />
        <attr name="label" format="reference" />
        <attr name="type" format="enum">
            <enum name="DANCEABILITY" value="0"/>
            <enum name="ENERGY" value="1"/>
            <enum name="SPEECHINESS" value="2"/>
            <enum name="ACOUSTICNESS" value="3"/>
            <enum name="INSTRUMENTALNESS" value="4"/>
            <enum name="LIVENESS" value="5"/>
            <enum name="VALENCE" value="6"/>
            <enum name="TEMPO" value="7"/>
        </attr>
    </declare-styleable>

</resources>